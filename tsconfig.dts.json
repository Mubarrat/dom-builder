{
	"compilerOptions": {
		"target": "ES2021",
		"module": "none",
		"outFile": "./dist/dom.js",
		"declaration": true,
		"declarationMap": true,
		"strict": true,
		"removeComments": false,
		"lib": ["DOM", "ES2021"],
		"forceConsistentCasingInFileNames": true,
    	"noImplicitAny": false,
		"skipLibCheck": true
	},
	"include": ["src/**/*"]
}
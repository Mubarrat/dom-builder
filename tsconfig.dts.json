{
	"compilerOptions": {
		"target": "ES2020",
		"module": "none",
		"outFile": "./dist/dom.js",
		"declaration": true,
		"declarationMap": true,
		"strict": true,
		"removeComments": false,
		"lib": ["DOM", "ES2020"],
		"forceConsistentCasingInFileNames": true,
    	"noImplicitAny": false,
		"skipLibCheck": true
	},
	"include": ["src/**/*"]
}